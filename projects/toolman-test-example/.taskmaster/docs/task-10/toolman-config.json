{
  "remoteTools": [
    "brave_web_search",
    "query_rust_docs",
    "get_github_readme",
    "getAPIResources",
    "describeResource",
    "listNamespaces",
    "getNamespacedResources"
  ],
  "localServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/tmp"],
      "tools": [
        "write_file",
        "read_file",
        "create_directory",
        "edit_file",
        "list_directory",
        "directory_tree",
        "file_search"
      ],
      "workingDirectory": "project_root"
    },
    "git": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-git"],
      "tools": [
        "git_status",
        "git_diff_unstaged",
        "git_log",
        "git_commit",
        "git_add"
      ],
      "workingDirectory": "project_root"
    }
  }
}