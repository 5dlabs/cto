{
  "remoteTools": [
    "brave-search_brave_web_search",
    "memory_create_entities",
    "memory_query_entities"
  ],
  "localServers": {
    "mcp-memory": {
      "command": "mcp-memory-server",
      "args": ["--storage-path", "/workspace/memory"]
    },
    "mcp-argo": {
      "command": "mcp-argo-client",
      "args": ["--server-url", "http://argo-server.argo.svc.cluster.local:2746"],
      "env": {
        "ARGO_SERVER": "argo-server.argo.svc.cluster.local:2746",
        "ARGO_NAMESPACE": "argo",
        "CONNECTION_TIMEOUT": "10"
      }
    }
  }
}