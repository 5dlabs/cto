{
  "remoteTools": [
    "rustdocs_query_rust_docs",
    "rustdocs_search_documentation",
    "rustdocs_get_crate_info",
    "brave-search_brave_web_search",
    "memory_create_entities",
    "memory_query_entities"
  ],
  "localServers": {
    "mcp-rustdocs": {
      "command": "mcp-rustdocs-client",
      "args": ["--server-url", "http://rustdocs-mcp-service.agent-platform.svc.cluster.local:8080"],
      "env": {
        "MCP_SERVER_URL": "http://rustdocs-mcp-service.agent-platform.svc.cluster.local:8080",
        "RUST_LOG": "info",
        "CONNECTION_TIMEOUT": "10",
        "RETRY_ATTEMPTS": "3"
      }
    },
    "mcp-memory": {
      "command": "mcp-memory-server",
      "args": ["--storage-path", "/workspace/memory"]
    }
  }
}