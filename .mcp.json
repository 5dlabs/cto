{
	"mcpServers": {
		"toolman": {
			"type": "stdio",
			"command": "toolman",
			"args": [
				"--working-dir",
				"/workspace"
			],
			"env": {
				"TOOLMAN_SERVER_URL": "http://toolman.agent-platform.svc.cluster.local:3000/mcp"
			}
		},
		"task-master-ai": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"--package=task-master-ai",
				"task-master-ai"
			]

		}
	}
}
